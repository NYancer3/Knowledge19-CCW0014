<name>sp_widget_88f58caf4f8417402debb7a18110c72c</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sp_widget"&gt;&lt;sp_widget action="INSERT_OR_UPDATE"&gt;&lt;category&gt;custom&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[function($rootScope,$scope,spUtil) {
	/* widget controller */
	var c = this;
	
	c.countThis = 0;
	
	c.rsb = function(){
		$rootScope.$broadcast('plus.one');
	};
	
	c.sb = function(){
		$scope.$broadcast('plus.one');
	};
	
	c.se = function(){
		$scope.$emit('plus.one');
	};
	
	$scope.$on('plus.one',function(evt,args){
		c.countThis++;
	});
	
	c.child = '';
	
	spUtil.get('scope-demo2').then(function(resp){
		c.child = resp;
	});
	
	$scope.$on('clear.counter',function(evt,args){
		c.countThis = 0;
	});
	
}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css/&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description&gt;Embedded in the main scope demo widget. Includes an embedded widget that allows you to add a child widget.&lt;/description&gt;&lt;docs/&gt;&lt;field_list/&gt;&lt;has_preview&gt;false&lt;/has_preview&gt;&lt;id&gt;scope-demo1&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link/&gt;&lt;name&gt;Scope Demo 1&lt;/name&gt;&lt;option_schema/&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[(function() {
  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */

})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2018-02-08 19:55:56&lt;/sys_created_on&gt;&lt;sys_id&gt;88f58caf4f8417402debb7a18110c72c&lt;/sys_id&gt;&lt;sys_mod_count&gt;6&lt;/sys_mod_count&gt;&lt;sys_name&gt;Scope Demo 1&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_88f58caf4f8417402debb7a18110c72c&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;nicholas.yancer&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2018-02-28 21:42:57&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;div&gt;
  &lt;h2&gt;Counter: {{ c.countThis }}&lt;/h2&gt;
  &lt;p&gt;Embedded in Scope Demo widget&lt;/p&gt;
  &lt;button class="btn btn-primary" ng-click="c.rsb()"&gt;RootScope - Broadcast&lt;/button&gt;
  &lt;button class="btn btn-primary" ng-click="c.sb()"&gt;Scope - Broadcast&lt;/button&gt;
  &lt;button class="btn btn-primary" ng-click="c.se()"&gt;Scope - Emit&lt;/button&gt;
  
  &lt;sp-widget ng-if="!!c.child" widget="c.child"&gt;&lt;/sp-widget&gt;
&lt;/div&gt;]]&gt;&lt;/template&gt;&lt;/sp_widget&gt;&lt;/record_update&gt;</payload>
